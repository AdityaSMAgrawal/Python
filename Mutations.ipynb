    "def mutate_string(string, position, character):\n",
    "    list2 = list(string)\n",
    "    list2.insert(position,character)\n",
    "    string = ''.join(list2)\n",
    "    return list2\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    s = input()\n",
    "    i, c = input().split()\n",
    "    s_new = mutate_string(s, int(i), c)\n",
    "    print(s_new)"
   ]
